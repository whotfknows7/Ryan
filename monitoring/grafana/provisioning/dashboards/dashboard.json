      "collapsed": false,
      "panels": []
    },
    {
      "title": "Top Processes by CPU",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus_ds"
      },
      "targets": [
        {
          "expr": "sort_desc(topk(10, sum by (groupname) (rate(namedprocess_namegroup_cpu_seconds_total[1m])) * 100))",
          "format": "table",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "CPU %",
              "groupname": "Process"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "desc": true,
                "field": "Value"
              }
            ]
          }
        }
      ]
    },
    {
      "title": "Top Processes by Memory",
