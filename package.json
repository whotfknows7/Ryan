{
  "name": "ryan-v5",
  "version": "1.0.9",
  "description": "Discord XP bot with Rust-powered rendering",
  "main": "Ryan/src/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "setup": "npm install && cd renderer && cargo build --release",
    "cleanup": "node cleanup.js",
    "start": "npx prisma db push --accept-data-loss && (cd Renderer && cargo build --release) && concurrently \"npm run start:renderer\" \"npm run start:bot\"",
    "start:bot": "node Ryan/src/index.js",
    "start:renderer": "cd renderer && cargo run --release",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:bot\"",
    "dev:bot": "node --watch --no-warnings Ryan/src/index.js",
    "dev:renderer": "cd renderer && cargo run",
    "bot-only": "npx prisma db push --accept-data-loss && node Ryan/src/index.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:setup": "prisma db push && prisma generate",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "discord",
    "bot",
    "xp",
    "leveling",
    "rust",
    "renderer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.7.9",
    "concurrently": "^8.2.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "emoji-regex": "^10.6.0",
    "ffmpeg-static": "^8.0.1",
    "gifsicle": "^5.3.0",
    "opentype.js": "^1.3.4",
    "pg": "^8.17.1",
    "sharp": "^0.33.5",
    "undici": "^7.18.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}