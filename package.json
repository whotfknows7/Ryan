{
  "name": "ryan-v5",
  "version": "1.0.9",
  "description": "Discord XP bot with Rust-powered rendering",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "setup": "npm install && cd renderer && cargo build --release",
    "cleanup": "node cleanup.js",
    "start": "npx prisma db push --accept-data-loss && (cd Renderer && cargo build --release) && concurrently \"npm run start:renderer\" \"npm run start:bot\"",
    "start:bot": "node src/index.js",
    "start:renderer": "cd renderer && cargo run --release",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:bot\"",
    "dev:bot": "node --watch --no-warnings src/index.js",
    "dev:renderer": "cd renderer && cargo run",
    "bot-only": "npx prisma db push --accept-data-loss && node src/index.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:setup": "prisma db push && prisma generate",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "discord",
    "bot",
    "xp",
    "leveling",
    "rust",
    "renderer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.7.9",
    "concurrently": "^8.2.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "emoji-regex": "^10.6.0",
    "gifencoder": "^2.0.1",
    "gifsicle": "^5.3.0",
    "opentype.js": "^1.3.4",
    "pg": "^8.17.1",
    "sharp": "^0.33.5",
    "undici": "^7.18.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^7.2.0"
  },
  "overrides": {
    "cross-spawn": "^7.0.6",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "ws": "^8.19.0",
    "tar": "^7.4.3",
    "cookie": "^0.7.2",
    "tough-cookie": "^5.0.0",
    "axios": "^1.7.9",
    "follow-redirects": "^1.15.9",
    "ip": "^2.0.1",
    "inflight": "^1.0.6",
    "glob": "^11.0.0",
    "rimraf": "^5.0.5",
    "path-to-regexp": "^0.1.12",
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "send": "^0.19.0",
    "serve-static": "^1.16.2",
    "qs": "^6.13.0",
    "nth-check": "^2.1.1",
    "semver": "^7.6.3",
    "debug": "^4.4.0",
    "uuid": "^10.0.0",
    "are-we-there-yet": "^4.0.2",
    "gauge": "^5.0.1",
    "npmlog": "^7.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}