{
  "name": "ryan-bot",
  "version": "2.0",
  "description": "Discord XP bot with Rust-powered rendering",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "setup": "npm install && cd Renderer && cargo build --release",
    "cleanup": "node cleanup.js",
    "start": "npx prisma db push --accept-data-loss && (cd Renderer && cargo build --release) && npm run start:bot",
    "start:bot": "node src/index.js",
    "start:renderer": "cd Renderer && cargo run --release",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:bot\"",
    "dev:bot": "node --watch --no-warnings src/index.js",
    "dev:renderer": "cd Renderer && cargo run",
    "bot-only": "npx prisma db push --accept-data-loss && node src/index.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:setup": "prisma db push && prisma generate",
    "db:studio": "prisma studio",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "discord",
    "bot",
    "xp",
    "leveling",
    "rust",
    "renderer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "bullmq": "^5.69.2",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "emoji-regex": "^10.6.0",
    "ioredis": "^5.9.3",
    "lru-cache": "^11.2.6",
    "opentype.js": "^1.3.4",
    "pg": "^8.17.1",
    "rate-limiter-flexible": "^9.1.1",
    "sharp": "^0.34.5",
    "winston": "^3.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.0.1",
    "prettier": "^3.8.1",
    "prisma": "^7.4.0"
  },
  "overrides": {
    "hono": "^4.11.7",
    "lodash": "^4.17.22",
    "cross-spawn": "^7.0.6",
    "http-cache-semantics": "^4.1.1",
    "semver-regex": "^4.0.5",
    "undici": "^6.23.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
